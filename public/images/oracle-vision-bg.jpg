<!-- This is a binary file, so we'll create an SVG that simulates an oracle vision background -->
<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="oracleGlow" cx="50%" cy="50%" r="50%" fx="50%" fy="50%">
      <stop offset="0%" stop-color="#9D72FF" stop-opacity="0.7" />
      <stop offset="70%" stop-color="#3A1078" stop-opacity="0.5" />
      <stop offset="100%" stop-color="#0F0F1B" stop-opacity="0.8" />
    </radialGradient>
    
    <filter id="noiseFilter">
      <feTurbulence type="fractalNoise" baseFrequency="0.04" numOctaves="5" stitchTiles="stitch" />
      <feColorMatrix type="matrix" values="0 0 0 0 0.6 0 0 0 0 0.4 0 0 0 0 0.8 0 0 0 0.1 0" />
    </filter>
    
    <filter id="blurFilter">
      <feGaussianBlur stdDeviation="10" />
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="#0F0F1B" />
  
  <!-- Oracle Eye -->
  <circle cx="600" cy="400" r="200" fill="url(#oracleGlow)" filter="url(#blurFilter)" />
  <circle cx="600" cy="400" r="80" fill="#D5B048" filter="url(#blurFilter)" opacity="0.7" />
  <circle cx="600" cy="400" r="40" fill="#F8F0E0" filter="url(#blurFilter)" opacity="0.9" />
  
  <!-- Rays -->
  <g opacity="0.6">
    <line x1="600" y1="100" x2="600" y2="300" stroke="#9D72FF" stroke-width="5" filter="url(#blurFilter)" />
    <line x1="600" y1="500" x2="600" y2="700" stroke="#9D72FF" stroke-width="5" filter="url(#blurFilter)" />
    <line x1="300" y1="400" x2="500" y2="400" stroke="#9D72FF" stroke-width="5" filter="url(#blurFilter)" />
    <line x1="700" y1="400" x2="900" y2="400" stroke="#9D72FF" stroke-width="5" filter="url(#blurFilter)" />
    <line x1="400" y1="200" x2="550" y2="350" stroke="#9D72FF" stroke-width="5" filter="url(#blurFilter)" />
    <line x1="800" y1="200" x2="650" y2="350" stroke="#9D72FF" stroke-width="5" filter="url(#blurFilter)" />
    <line x1="400" y1="600" x2="550" y2="450" stroke="#9D72FF" stroke-width="5" filter="url(#blurFilter)" />
    <line x1="800" y1="600" x2="650" y2="450" stroke="#9D72FF" stroke-width="5" filter="url(#blurFilter)" />
  </g>
  
  <!-- Noise Overlay -->
  <rect width="100%" height="100%" fill="transparent" filter="url(#noiseFilter)" opacity="0.3" />
  
  <!-- Ancient Symbols -->
  <g fill="#D5B048" opacity="0.4" filter="url(#blurFilter)">
    <text x="300" y="200" font-family="serif" font-size="40">Ω</text>
    <text x="850" y="300" font-family="serif" font-size="40">Δ</text>
    <text x="400" y="650" font-family="serif" font-size="40">Φ</text>
    <text x="800" y="550" font-family="serif" font-size="40">Θ</text>
    <text x="200" y="400" font-family="serif" font-size="40">Σ</text>
    <text x="950" y="450" font-family="serif" font-size="40">Ψ</text>
  </g>
</svg> 